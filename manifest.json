{
  "name": "Top Bar for Trello.com",
  "description": "Hide and show lists at Trello.com site.",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [ {
    "css": ["style.css"],
    "js": [ 
      "js/jquery-1.8.0.min.js", 
      "js/content.js" 
    ],
    "matches": [
      "https://trello.com/*"
    ]
  } ],
  "icons": {
    "128": "icons/icon_128.png",
    "48": "icons/icon_48.png",
    "32": "icons/icon_32.png",
    "16": "icons/icon_16.png"
  }
}
